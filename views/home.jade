extends layout

block content

  div.home-rules-buffer
  div.home-rules.tmg-trans-0.tmg-unselectable

    //- div.rules-bg.tmg-trans-80.gradient.radial-gradient
    div.home-rules-number-stretch.rule-number= "Rule #00"
    hr.non-mobile-only.rule-hr(style="top:-120px;")
    hr.mobile-only.rule-hr(style="top:-125px;")
    div.home-rules-body-squeeze.rule-body
      span.brackets= "[ "
      span.rule-body-inner= "RULE"
      span.brackets= " ]"
    hr.non-mobile-only.rule-hr(style="top:-210px;")
    hr.mobile-only.rule-hr(style="top:-200px;")
    span.rule-footer= "HOW TO DO REALLY GREAT WORK"

